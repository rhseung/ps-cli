{
  "name": "@rhseung/ps-cli",
  "version": "1.1.0",
  "description": "백준(BOJ) 문제 해결을 위한 통합 CLI 도구",
  "type": "module",
  "bin": {
    "ps": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup && node scripts/add-shebang.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "release:patch": "npm version patch && bun publish",
    "release:minor": "npm version minor && bun publish",
    "release:major": "npm version major && bun publish"
  },
  "keywords": [
    "boj",
    "baekjoon",
    "algorithm",
    "cli",
    "problem-solving"
  ],
  "author": "rhseung <ryu@rhseung.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhseung/ps-cli.git"
  },
  "homepage": "https://github.com/rhseung/ps-cli#readme",
  "bugs": {
    "url": "https://github.com/rhseung/ps-cli/issues"
  },
  "dependencies": {
    "@types/gradient-string": "^1.1.6",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "chokidar": "^3.6.0",
    "conf": "^11.0.2",
    "execa": "^9.4.0",
    "gradient-string": "^2.0.2",
    "ink": "^5.0.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "meow": "^14.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
